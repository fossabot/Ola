{
  "name": "@marketgoo/ola",
  "version": "0.1.2-beta.1",
  "description": "Design System by marketgoo",
  "main": "dist/index.js",
  "styles": "dist/index.css",
  "license": "MIT",
  "scripts": {
    "start": "concurrently npm:watch npm:guide",
    "lint": "eslint ./src",
    "watch": "rm -rf dist && mkdir dist && concurrently npm:watch:js npm:watch:css",
    "watch:js": "babel ./src -d dist --watch",
    "watch:css": "postcss src/index.css -o dist/index.css --watch",
    "guide": "start-storybook -p 6006 -s ./static",
    "build": "rm -rf dist && mkdir dist && babel ./src -d dist && postcss src/index.css -o dist/index.css",
    "build:storybook": "build-storybook",
    "publish:storybook": "gh-pages -d storybook-static"
  },
  "homepage": "https://github.com/marketgoo/Ola.git",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/marketgoo/Ola.git"
  },
  "files": [
    "dist",
    "src",
    "*.md",
    "ola.png",
    "LICENSE"
  ],
  "peerDependencies": {
    "react": "^16.8.6",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.1.1",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.1.1",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-prefix-selector": "^1.7.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  }
}
