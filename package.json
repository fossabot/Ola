{
  "name": "@ola/components",
  "version": "0.1.0",
  "description": "Design System by marketgoo",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently npm:watch npm:guide",
    "lint": "eslint ./src",
    "watch": "webpack -d --watch --config ./webpack.config.js --display-error-details",
    "guide": "start-storybook -p 6006 -s ./static",
    "build": "npm run build:commonjs && npm run build:umd",
    "build:commonjs": "mkdir -p lib && babel ./src -d lib",
    "build:umd": "webpack",
    "build:storybook": "build-storybook",
    "prepack": "npm run build"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "*.md",
    "ola.png"
  ],
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-prefix-selector": "^1.7.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {}
}
